# In .env
DB_HOST=localhost # If Go app runs on host, connecting to Dockerized DB
DB_PORT=5432
DB_USER=your_user # e.g., seattle_user
DB_PASSWORD=your_strong_password
DB_NAME=seattle_info_dev
DB_SSL_MODE=disable
DB_TIMEZONE=UTC
# For golang-migrate CLI (ensure user/pass/db match above)
DB_SOURCE="postgresql://your_user:your_strong_password@localhost:5432/seattle_info_dev?sslmode=disable"